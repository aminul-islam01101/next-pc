{
  "name": "next-pc-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx",
    "prettier:check": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|ts|tsx|json)\"",
    "lint:fix": "eslint . --fix",
    "prettier:fix": "prettier --write \"src/**/*.+(js|ts|tsx|json)\"",
    "lint-prettier-fix": "yarn lint:fix && yarn prettier:fix  && yarn lint",
    "lint-prettier-check": "yarn lint:check && yarn prettier:check",
    "lint-prettier": "yarn lint-prettier-fix && yarn lint-prettier-check"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": "yarn lint-prettier"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.3.0",
    "@ant-design/icons": "^5.1.4",
    "@types/node": "20.4.4",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "antd": "^5.7.3",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "json-server": "^0.17.3",
    "mongoose": "^7.4.1",
    "next": "13.4.12",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "@reduxjs/toolkit": "^1.9.5",
    "react-redux": "^8.1.1",
    "axios": "^1.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  }
}
